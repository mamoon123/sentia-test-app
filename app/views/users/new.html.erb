<h3><center>Add New User</center></h3>
<div>
  <h4 class='import-text'>Import the Data </h4>
  <%= form_tag import_users_path, multipart: true , class: 'import-form' do %> 
    <%= file_field_tag 'user_file', required: true, accept: ".csv" %>
    <%= submit_tag 'Import CSV', class: 'btn-default btn-warning'%>
  <% end %> 
</div>
<hr/>

<%= render 'form', user: @user %>

